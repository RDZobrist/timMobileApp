#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/typescript@4.3.5/node_modules/typescript/bin/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/typescript@4.3.5/node_modules/typescript/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/typescript@4.3.5/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules:/Users/Zobrist/Desktop/Code/work/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/typescript@4.3.5/node_modules/typescript/bin/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/typescript@4.3.5/node_modules/typescript/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/typescript@4.3.5/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules/.pnpm/node_modules:/Users/Zobrist/Desktop/Code/work/tim_church_mobileapp/node_modules:/Users/Zobrist/Desktop/Code/work/node_modules:/Users/Zobrist/Desktop/Code/node_modules:/Users/Zobrist/Desktop/node_modules:/Users/Zobrist/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsserver" "$@"
else
  exec node  "$basedir/../../bin/tsserver" "$@"
fi
